{
    "manifest_version": 2,
    "name": "Basic",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "browser_action": {
        "scripts": ["background.js"]
    }
}